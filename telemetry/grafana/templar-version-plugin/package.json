
{
  "name": "templar-version-plugin",
  "version": "1.0.0",
  "description": "Templar Version Plugin for Grafana",
  "scripts": {
    "build": "webpack --env production",
    "dev": "webpack --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "e2e": "playwright test -c playwright.config.ts",
    "e2e:update": "playwright test -c playwright.config.ts --update-snapshots",
    "server": "docker-compose up --build"
  },
  "author": "Templar Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "8.42.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "webpack": "5.85.0",
    "webpack-cli": "5.1.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@emotion/css": "11.11.0",
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.0",
    "@grafana/ui": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  }
}
